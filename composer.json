{
    "name": "wakeworks/elemental-animator",
    "description": "Adds animation options to your elements",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "elemental",
        "animations"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^4.4 || ^5",
        "dnadesign/silverstripe-elemental": "^4 || ^5"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3"
    },
    "autoload": {
        "psr-4": {
            "WakeWorks\\ElementalAnimator\\": "src/",
            "WakeWorks\\ElementalAnimator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "expose": [
            "client/dist"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
